{
  "name": "@sap/cds",
  "version": "6.8.4",
  "description": "SAP Cloud Application Programming Model - CDS for Node.js",
  "homepage": "https://cap.cloud.sap/",
  "keywords": [
    "CAP",
    "CDS"
  ],
  "author": "SAP SE (https://www.sap.com)",
  "license": "SEE LICENSE IN LICENSE",
  "typings": "apis/cds.d.ts",
  "main": "lib/index.js",
  "bin": {
    "cds": "bin/cds.js",
    "cds-serve": "bin/cds-serve.js"
  },
  "files": [
    "apis/",
    "app/",
    "bin/",
    "lib/",
    "libx/",
    "tasks/",
    "srv/",
    "_i18n/",
    "server.js",
    "common.cds",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@sap/cds-compiler": "^3.2.0",
    "@sap/cds-foss": "^4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "libx/odata/grammar.pegjs": [
      "npm run pegjs:odata2cqn && git add libx/odata/parser.js"
    ],
    "{libx,tests/_runtime}/**/*.js": [
      "npx prettier --write"
    ]
  }
}