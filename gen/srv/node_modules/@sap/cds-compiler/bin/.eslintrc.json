{
  "extends": ["../.eslintrc-ydkjsi.json"],
  "rules": {
    "prefer-const": "error",
    "quotes": ["error", "single", "avoid-escape"],
    "prefer-template": "error",
    "no-trailing-spaces": "error",
    "template-curly-spacing": ["error", "never"],
    "max-len": "off",
    "no-console": "off",
    "no-process-exit": "off",
    "camelcase": "off",
    "radix": "off",
    "no-shadow": "warn",
    "global-require": "off"
  }
}
