COLUMN TABLE carbon_Habits (
  ID NVARCHAR(36) NOT NULL,
  habitCategory_ID NVARCHAR(36),
  code NVARCHAR(5000),
  tag NVARCHAR(5000),
  option NVARCHAR(5000),
  additionalInformation NVARCHAR(5000),
  factor DOUBLE,
  default BOOLEAN,
  PRIMARY KEY(ID)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN carbon_HabitCategories AS habitCategory ON (habitCategory.ID = habitCategory_ID)
)