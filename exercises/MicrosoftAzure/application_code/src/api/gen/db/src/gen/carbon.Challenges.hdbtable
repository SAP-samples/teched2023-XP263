COLUMN TABLE carbon_Challenges (
  ID NVARCHAR(36) NOT NULL,
  title NVARCHAR(5000),
  description NCLOB,
  icon NVARCHAR(5000),
  category NVARCHAR(5000),
  mcc_ID NVARCHAR(36),
  daysToMark INTEGER,
  avoidableEmissionsPerDay DOUBLE,
  PRIMARY KEY(ID)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN carbon_MCC AS mcc ON (mcc.ID = mcc_ID)
)