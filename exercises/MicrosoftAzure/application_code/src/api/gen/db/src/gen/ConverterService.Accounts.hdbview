VIEW ConverterService_Accounts AS SELECT
  Accounts_0.ID,
  Accounts_0.name
FROM carbon_Accounts AS Accounts_0
WITH ASSOCIATIONS (
  MANY TO MANY JOIN ConverterService_Transactions AS transactions ON (transactions.account_ID = ID),
  MANY TO MANY JOIN ConverterService_ChallengesUsers AS challenges ON (challenges.account_ID = ID),
  MANY TO MANY JOIN ConverterService_AccountHabits AS habits ON (habits.account_ID = ID)
)