VIEW ConverterService_HabitCategories AS SELECT
  HabitCategories_0.ID,
  HabitCategories_0.question,
  HabitCategories_0.additionalInformation,
  HabitCategories_0.icon,
  HabitCategories_0.category
FROM carbon_HabitCategories AS HabitCategories_0
WITH ASSOCIATIONS (
  MANY TO MANY JOIN ConverterService_Habits AS options ON (options.habitCategory_ID = ID),
  MANY TO MANY JOIN ConverterService_HabitCategoriesMCC AS mccs ON (mccs.habitCategory_ID = ID)
)