VIEW ConverterService_Challenges AS SELECT
  Challenges_0.ID,
  Challenges_0.title,
  Challenges_0.description,
  Challenges_0.icon,
  Challenges_0.category,
  Challenges_0.mcc_ID,
  Challenges_0.daysToMark,
  Challenges_0.avoidableEmissionsPerDay
FROM carbon_Challenges AS Challenges_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN ConverterService_MCC AS mcc ON (mcc.ID = mcc_ID)
)