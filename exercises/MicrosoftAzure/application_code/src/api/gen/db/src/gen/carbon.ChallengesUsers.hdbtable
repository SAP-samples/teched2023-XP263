COLUMN TABLE carbon_ChallengesUsers (
  ID NVARCHAR(36) NOT NULL,
  account_ID NVARCHAR(36),
  challenge_ID NVARCHAR(36),
  startDate DATE,
  dueDate DATE,
  markedDates NCLOB,
  isCompleted BOOLEAN,
  PRIMARY KEY(ID)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN carbon_Accounts AS account ON (account.ID = account_ID),
  MANY TO ONE JOIN carbon_Challenges AS challenge ON (challenge.ID = challenge_ID)
)