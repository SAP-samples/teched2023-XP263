COLUMN TABLE carbon_HabitCategories (
  ID NVARCHAR(36) NOT NULL,
  question NVARCHAR(5000),
  additionalInformation NVARCHAR(5000),
  icon NVARCHAR(5000),
  category NVARCHAR(5000),
  PRIMARY KEY(ID)
) WITH ASSOCIATIONS (
  MANY TO MANY JOIN carbon_Habits AS options ON (options.habitCategory_ID = ID),
  MANY TO MANY JOIN carbon_HabitCategoriesMCC AS mccs ON (mccs.habitCategory_ID = ID)
)